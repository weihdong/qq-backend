{
    "builds": [
      {
        "src": "server.js",
        "use": "@railway/node",
        "config":{
          "runtime": "node",
          "engines": {
            "node": ">=18"
          }
        }
      }
    ],
    "routes": [
      {
        "handle": "all",
        "src": ".*",
        "dest": "/"
      }
    ]
  }